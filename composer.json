{
    "type": "library",
    "name": "autoframe/components-env",
    "description": "PHP Environment Tools, Autoframe Framework",
    "license": "MIT",
    "homepage": "https://github.com/autoframe",
    "require": {
        "php": "^7.4 || ^8.0",
        "autoframe/components-exception": "^2.0",
        "autoframe/components-filesystem": "^2.0.3",
        "autoframe/design-patterns": "^1.0.1",
        "autoframe/components-arr": "^2.0.1"
    },
    "version": "1.0.0",
    "authors": [
        {
            "name": "Nistor Alexandru Marius",
            "homepage": "https://autoframe.ro",
            "role": "Developer"
        }
    ],
    "keywords": [
        "Env",
        "Envoirement",
        "Parser",
        "getenv",
        "putenv"
    ],
    "autoload": {
        "psr-4": {
            "Autoframe\\Components\\Env\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unit\\": "Tests/Unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10"
    },
    "optimize-autoloader": true
}
